<body>

<!-- Navbar -->  

 <%- include("COMPANY/OWNER/PARTIALS/header") %>
<!-- Navbar end -->


<!-- Start Landing Page -->

<div class="banner-image py-5">
  <div class="container py-5">
    <div class="row py-5 align-items-center">
      <div class="col-lg-6">
        <h5 class="display-4 mb-4 font-weight-bold text-center text-lg-left">Stay connected with your customers</h5> 
          <p class="lead mb-5 text-black-50 text-center text-lg-left">your workers data will be send on your email within 24 hrs</p> 
          <div class="d-flex justify-content-center justify-content-lg-start mb-4">
        </div>
                
      </div>
      <div class="col-lg-6 text-center mt-5 mt-lg-0">
        <div class="lead-form rounded p-5"> 
        <form id="contact-form">
                                <div>
                                    <h3 class="font-weight-bold">Get started for FREE!</h3>
                                    <h6 class="text-black-50 pt-3 pb-4 small font-weight-bold">Fill all fields so we can get some info about you.<br>We'll never send you spam</h6>
                                </div>
                                <div class="row">
                                    <div class="col-12">
                                        <div class="form-group">
                                            <input type="text" class="form-control form-control-lg" name="Companyname" placeholder="Company Name" required>
                                        </div>
                                        <div class="form-group">
                                            <input type="email" class="form-control form-control-lg" name="email" placeholder="Email" required>
                                        </div>
                                        <div class="form-group">
                                            <input type="text" class="form-control form-control-lg" name="State" placeholder="State" required>
                                        </div>
                                    </div>
                                    <div class="col-12">
                                        <button class="btn btn-danger btn-lg w-100 mt-3 mt-sm-4" type="submit">Send Query <i class="lni lni-arrow-right ml-2"></i></button>
                                        <div>
                                            <span class="d-inline-block mt-4 text-black-50 small">By signing up, you accept our <a href="#">Terms</a> &amp; <a href="#">Privacy Policy</a></span>
                                        </div>
                                    </div>
                                </div>
                            </form>
         </div>
      </div>
    </div>
  </div>
</div>
 
